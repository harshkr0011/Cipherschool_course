// q12.js
class Person {
  constructor(name, age) {
    this.name = name;
    this.age = age;
  }

  sayHello() {
    console.log(`Hello, my name is ${this.name} and I am ${this.age} years old.`);
  }
}

const person2 = new Person("Bob", 25);
person2.sayHello(); // Output: Hello, my name is Bob and I am 25 years old.
